{
  "sig": {
    "prefix": ["sig"],
    "body": [
      "sig { $1 }$2"
    ],
    "description": "Sorbet signature"
  },
  "sig block": {
    "prefix": ["sig block"],
    "body": [
      "sig do",
      "  $1",
      "end$2"
    ],
    "description": "Sorbet signature block"
  },
  "require": {
    "prefix": ["require"],
    "body": [
      "require \"$1\"$2"
    ],
    "description": "Better require statement"
  },
  "frozen string literal": {
    "prefix": ["frozen"],
    "body": [
      "$LINE_COMMENT frozen_string_literal: true",
      "$2"
    ],
    "description": "Frozen string literal header"
  },
  "typed": {
    "prefix": ["typed"],
    "body": [
      "$LINE_COMMENT typed: $1",
      "$2"
    ],
    "description": "Sorbet type header"
  }
}
